{{> header}}

<article class="post page">
  <header class="post-header">
    <a id="blog-logo" href="index.html">
      <img src="/assets/images/2014/Aug/BirdName-square.jpg" alt="Blog Logo"/>
    </a>
  </header>
  <h1 class="post-title">{{title}}</h1>
  <section class="post-content">
    {{#if image}}
      <img class="post-featured-image" src="{{image}}">
    {{/if}}

    {{{contents}}}
  </section>
</article>

{{> footer}}
